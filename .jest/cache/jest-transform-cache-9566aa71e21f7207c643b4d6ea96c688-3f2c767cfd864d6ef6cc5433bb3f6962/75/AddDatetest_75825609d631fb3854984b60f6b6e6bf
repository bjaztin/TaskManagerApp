var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _AddDate = _interopRequireDefault(require("../../../src/components/TaskScreenFeatures/AddDate"));
var _jsxRuntime = require("react/jsx-runtime");
describe("AddDate component", function () {
  test("Render AddDate function correctly", function () {
    var selectedDate = "2023-09-03";
    var onSelectedDateChange = jest.fn();
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_AddDate.default, {
        selectedDate: selectedDate,
        onSelectedDateChange: onSelectedDateChange
      })),
      getByTestId = _render.getByTestId;
    expect(getByTestId("undefined.day_2023-09-03")).toBeDefined();
  });
  test("Date is change when a day is pressed", function () {
    var selectedDate = "2023-09-03";
    var onSelectedDateChange = jest.fn();
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_AddDate.default, {
        selectedDate: selectedDate,
        onSelectedDateChange: onSelectedDateChange
      })),
      getByText = _render2.getByText;
    _reactNative.fireEvent.press(getByText("3"));
    expect(onSelectedDateChange).toHaveBeenCalledWith("2023-09-03");
  });
});
