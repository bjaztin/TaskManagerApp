var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _OnBoarding = _interopRequireDefault(require("../../src/screens/OnBoarding"));
var _jsxRuntime = require("react/jsx-runtime");
describe("OnBoarding Component", function () {
  it("renders correctly", function () {
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_OnBoarding.default, {})),
      getByText = _render.getByText,
      getByTestId = _render.getByTestId;
    expect(getByTestId("Taskly")).toBeTruthy();
    expect(getByText("Efficient task management at your fingertips")).toBeTruthy();
    expect(getByTestId("getStarted")).toBeTruthy();
  });
  it("navigates to LogIn screen when 'Get Started' is pressed", function () {
    var navigation = {
      navigate: jest.fn()
    };
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_OnBoarding.default, {
        navigation: navigation
      })),
      getByTestId = _render2.getByTestId;
    _reactNative.fireEvent.press(getByTestId("getStarted"));
    expect(navigation.navigate).toHaveBeenCalledWith("LogIn");
  });
});
