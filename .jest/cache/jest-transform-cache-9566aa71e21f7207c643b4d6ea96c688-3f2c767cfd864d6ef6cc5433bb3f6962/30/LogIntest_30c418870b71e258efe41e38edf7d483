var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _LogIn = _interopRequireDefault(require("../../src/screens/LogIn"));
var _jsxRuntime = require("react/jsx-runtime");
describe("LogIn Component", function () {
  it("renders the LogIn screen correctly", function () {
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_LogIn.default, {})),
      getByTestId = _render.getByTestId,
      getByText = _render.getByText;
    expect(getByText("Sign In")).toBeTruthy();
    expect(getByTestId("email-input")).toBeTruthy();
    expect(getByTestId("password-input")).toBeTruthy();
    expect(getByTestId("login-button")).toBeTruthy();
  });
  it("Handle users input", function () {
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_LogIn.default, {})),
      getByTestId = _render2.getByTestId;
    _reactNative.fireEvent.changeText(getByTestId("email-input"), "admin@example.com");
    _reactNative.fireEvent.changeText(getByTestId("password-input"), "adminpassword");
    _reactNative.fireEvent.press(getByTestId("login-button"));
  });
});
