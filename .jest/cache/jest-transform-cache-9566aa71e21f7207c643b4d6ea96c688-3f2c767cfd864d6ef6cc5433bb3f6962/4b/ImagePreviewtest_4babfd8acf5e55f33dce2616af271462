var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _ImagePreview = _interopRequireDefault(require("../../src/components/TaskScreenFeatures/ImagePreview"));
var _jsxRuntime = require("react/jsx-runtime");
describe("ImagePreview component", function () {
  test("Render ImagePreview function correctly", function () {
    var onCloseMock = jest.fn();
    var imageUrl = "https://example.com/photo.jpg";
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ImagePreview.default, {
        visible: true,
        imageUrl: imageUrl,
        onClose: onCloseMock
      })),
      getByTestId = _render.getByTestId,
      getByText = _render.getByText;
    var imagePreview = getByTestId("imagePreview");
    expect(imagePreview).toBeTruthy();
    var buttonClose = getByTestId("buttonClose");
    expect(buttonClose).toBeTruthy();
    _reactNative.fireEvent.press(buttonClose);
    expect(onCloseMock).toHaveBeenCalledTimes(1);
  });
});
