var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _TaskImage = _interopRequireDefault(require("../../../src/components/TaskDetail/TaskImage"));
var _jsxRuntime = require("react/jsx-runtime");
describe("TaskImage component", function () {
  test("Renders TaskImage function correctly", function () {
    var onImagePress = jest.fn();
    var selectedImage = "https://image123.com/photo.jpg";
    var isComplete = false;
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_TaskImage.default, {
        selectedImage: selectedImage,
        isComplete: isComplete,
        onImagePress: onImagePress
      })),
      getByTestId = _render.getByTestId;
    var taskImage = getByTestId("taskImage");
    expect(taskImage).toBeDefined();
    _reactNative.fireEvent.press(taskImage);
    expect(onImagePress).toHaveBeenCalledWith(selectedImage);
  });
});
