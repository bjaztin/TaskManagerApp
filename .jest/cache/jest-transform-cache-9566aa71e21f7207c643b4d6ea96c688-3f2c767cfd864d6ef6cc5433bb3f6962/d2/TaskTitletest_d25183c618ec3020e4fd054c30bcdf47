var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _TaskTitle = _interopRequireDefault(require("../../../src/components/TaskDetail/TaskTitle"));
var _jsxRuntime = require("react/jsx-runtime");
describe("TaskTitle component", function () {
  test("Render TaskTitle function correctly", function () {
    var task = "This is task";
    var isComplete = false;
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_TaskTitle.default, {
        task: task,
        isComplete: isComplete
      })),
      getByText = _render.getByText;
    var taskTitle = getByText(task);
    expect(taskTitle).toBeDefined();
  });
  test("When task is complete, change the text to gray", function () {
    var task = "This task is completed";
    var isComplete = true;
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_TaskTitle.default, {
        task: task,
        isComplete: isComplete
      })),
      getByText = _render2.getByText;
    var taskTitle = getByText(task);
    expect(taskTitle).toBeDefined();
    var textStyleComplete = taskTitle.props.style[1].color;
    expect(textStyleComplete).toBe("gray");
  });
});
